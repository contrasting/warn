{
  "manifest_version": 3,
  "name": "Warn",
  "version": "1.0",
  "description": "Stops me from visiting stupid time wasting websites",
  "content_scripts": [
    {
      "js": ["content.js"],
      "matches": [
        "https://youtube.com/*",
        "https://www.youtube.com/*"
      ]
    }
  ],
  "background": {
    "service_worker": "service_worker.js"
  }
}